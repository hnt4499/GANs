{
    "data_loader": {
        "type": "data_loader",
        "name": "CelebALoader",
        "args":{
            "dataset": {
                "type": "dataset",
                "name": "CelebADataset",
                "args": {
                    "root": "/media/jupyter/Data/dataset/celeba",
                    "transform": {
                        "type": "pre_processing",
                        "name": "DCGAN_transform",
                        "args": {"image_size": 64, "mean": 0.5, "std": 0.5}
                    }
                }
            },
            "batch_size": 128,
            "num_workers": 4,
            "shuffle": false
        }
    }
}
