{
    "name": "DCGAN",
    "n_gpu": 1,
    "seed": 136,

    "trainer": {
        "ignored": true,
        "type": "trainer",
        "name": "BaseGANTrainer",
        "args": {
            "netD": {
                "type": "model",
                "name": "DCGANDiscriminator",
                "args": {
                    "image_size": 64,
                    "num_features": 64,
                    "num_channels": 3,
                    "conv_bias": false,
                    "negative_slope": 0.2,
                    "optimizer": {
                        "type": "optimizer",
                        "name": "Adam",
                        "args": {"lr": 0.0001, "betas": [0.5, 0.999], "weight_decay": 0, "amsgrad": false}
                    },
                    "criterion": {
                        "type": "criterion",
                        "name": "bce_loss",
                        "args": {}
                    },
                    "weights_init": {
                        "type": "weights_init",
                        "name": "DCGAN_wi",
                        "args": {"conv_mean": 0.0, "conv_std": 0.02, "bn_mean": 1.0, "bn_std": 0.02}
                    }
                }
            },
            "netG": {
                "type": "model",
                "name": "DCGANGenerator",
                "args": {
                    "image_size": 64,
                    "input_length": 100,
                    "num_features": 64,
                    "num_channels": 3,
                    "conv_bias": false,
                    "optimizer": {
                        "type": "optimizer",
                        "name": "Adam",
                        "args": {"lr": 0.0001, "betas": [0.5, 0.999], "weight_decay": 0, "amsgrad": true}
                    },
                    "criterion": {
                        "type": "criterion",
                        "name": "bce_loss",
                        "args": {}
                    },
                    "weights_init": {
                        "type": "weights_init",
                        "name": "DCGAN_wi",
                        "args": {"conv_mean": 0.0, "conv_std": 0.02, "bn_mean": 1.0, "bn_std": 0.02}
                    }
                }
            },
            "metrics": {
                "0": {
                    "type": "metric",
                    "name": "KIDScore",
                    "args": {
                        "model": {
                            "type": "features_extractor",
                            "name": "InceptionV3FE",
                            "args": {"output_layer": "Mixed_6e", "resize_to": null, "normalize_input": true,
                                     "batch_size": 128, "device": "cuda", "prune": false, "traverse_level": 1}
                        },
                        "name": "kid_inception", "max_samples": null, "kid_batch_size": 512, "dp": 4
                    }
                },
                "1": {
                    "type": "metric",
                    "name": "KIDScore",
                    "args": {
                        "model": {
                            "type": "features_extractor",
                            "name": "LeNet5FE",
                            "args": {"model_path": "compile/metrics_utils/features_extractors/lenet.pth", "resize_to": null,
                                     "output_layer": "convnet.tanh5", "batch_size": 128, "device": "cuda", "prune": true, "traverse_level": null}
                        },
                        "name": "kid_lenet", "max_samples": null, "kid_batch_size": 512, "dp": 4
                    }
                },
                "comment": "This protocol is used to handle arguments as a list of objects.",
                "comment_2": " Each metric should associate with a 0-based-index key, in the form of str, for example, \"0\", \"1\"."
            },

            "data_loader": {
                "type": "data_loader",
                "name": "ImageNetLoader",
                "args":{
                    "dataset": {
                        "type": "dataset",
                        "name": "ImageNetDataset",
                        "args": {
                            "root": "/media/jupyter/Data/dataset/ImageNet/ImageNet",
                            "cls": "n01514668",
                            "image_size": 64,
                            "include_val": true,
                            "transform": {
                                "type": "pre_processing",
                                "name": "DCGAN_transform",
                                "args": {"image_size": 64, "mean": 0.0, "std": 1.0}
                            }
                        }
                    },
                    "batch_size": 128,
                    "num_workers": 4,
                    "shuffle": true
                }
            },

            "callbacks": {
                "type": "callbacks",
                "name": "Thresholding",
                "args": {"monitor": "D_G_z2", "mode": "max", "threshold": 0.001, "min_delta": 0, "patience": 15, "verbose": 1}
            }
        },

        "epochs": 1000,

        "save_dir": "/media/jupyter/Data/train/DCGAN",
        "save_ckpt_every": 25,
        "num_ckpt_to_keep": 5,
        "save_images_every": 5,
        "write_logs_every": 1,
        "verbosity": 2,

        "tensorboard": true
    }
}
